"LightmappedGeneric"
{
	"%tooltexture" "custom/MClava"
	"%compileSlime" "1"
	//"%compileKeepLight" "1" //???

	"$basetexture" "custom/MClava"
	"$surfaceprop" "slime"
	"$selfillum" "1"

	//"$abovewater" "1"
	"$bottommaterial" "custom/MClava_beneath"

	"$fogenable" 1
	"$fogcolor" "{245 80 26}"
	"$fogstart" "-500"
	"$fogend" "200"

	//"$scroll1" "[.01 .01 .05]"
	//"$scroll2" "[-.025 .025 .075]"

	"Proxies"
	{

		"AnimatedTexture"
		{
			"animatedtexturevar" "$basetexture"
			"animatedtextureframenumvar" "$frame"
			"animatedtextureframerate" 5.00
		}

		"TextureScroll"
		{
			"texturescrollvar" "$baseTextureTransform"
			"texturescrollrate" 0.1
			"texturescrollangle" 30.00

		}
		//"WaterLOD"
		//{
		//}
	}
}

